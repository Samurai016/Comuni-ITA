      "get": {
        "summary": "Ottieni la lista di tutti i comuni italiani.",
        "operationId": "comuni",
        "tags": [
          "comuni"
        ],
        "responses": {
          "200": {
            "description": "L'operazione ha avuto successo.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Comune"
                  }
                }
              },
              "application/xml": {
                "schema": {
                  "type": "object",
                  "xml": {
                    "name": "comuni"
                  },
                  "properties": {
                    "page": {
                      "xml": {
                        "attribute": true
                      },
                      "type": "integer",
                      "description": "Il numero della pagina fornita",
                      "minimum": 1,
                      "example": 107
                    },
                    "pagesize": {
                      "xml": {
                        "attribute": true
                      },
                      "type": "integer",
                      "description": "La dimensione di ogni pagina.",
                      "minimum": 0,
                      "maximum": 500,
                      "default": 500
                    },
                    "total": {
                      "xml": {
                        "attribute": true
                      },
                      "type": "integer",
                      "description": "Il numero totale di entries disponibili.",
                      "minimum": 0,
                      "example": 500
                    },
                    "comuni": {
                      "type": "array",
                      "xml": {
                        "name": "comune"
                      },
                      "items": {
                        "$ref": "#/components/schemas/Comune"
                      }
                    }
                  }
                }
              },
              "text/csv": {
                "schema": {
                  "type": "string",
                  "description": "Lista CSV delimitata dal carattere ;  \nInclude l'header nella prima riga.  \nGli attributi che nei formati JSON e XML sono innestati, in CSV vengono convertiti in colonne nel formato\ncolonna_attributo",
                  "example": "codice;nome;nomeStraniero;codiceCatastale;cap;prefisso;provincia_nome;provincia_regione;email;pec;telefono;fax;coordinate_lat;coordinate_lng\n028001;Abano Terme;;A001;35031;049;Padova;Veneto;protocollo@abanoterme.net;abanoterme.pd@cert.ip-veneto.net;+39 049 8245111;+39 049 8600499;45.3619;11.7924"
                }
              }
            }
          },
          "5XX": {
            "description": "Errore del server. **Contattare lo sviluppatore.**"
          }
        }
      }
    },
